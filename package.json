{
  "name": "babel-preset-foray1010",
  "version": "7.10.2",
  "engines": {
    "node": ">=8.9"
  },
  "description": "my own babel preset",
  "keywords": [
    "babel",
    "preset"
  ],
  "private": false,
  "license": "MIT",
  "author": "foray1010",
  "homepage": "https://github.com/foray1010/babel-preset-foray1010",
  "repository": "foray1010/babel-preset-foray1010",
  "bugs": "https://github.com/foray1010/babel-preset-foray1010/issues",
  "main": "react.js",
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "ignore-sync": "ignore-sync",
    "lint": "eslint .",
    "precommit": "lint-staged"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "app-root-path": "^2.0.1",
    "babel-plugin-ramda": "1.6.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.0",
    "eslint": "^5.0.0",
    "eslint-config-foray1010": "^6.1.0",
    "husky": "^0.14.0",
    "ignore-sync": "^1.0.0",
    "lint-staged": "^7.0.0",
    "prettier-eslint-cli": "^4.3.2"
  }
}
